{
  "name": "techforge-communication",
  "version": "1.0.0",
  "description": "Real-time communication system for TechForge AI Agents",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "node server/index.js",
    "server": "node server/index.js",
    "agent": "node agents/techforge-spawn.js",
    "bridge": "node bridge/techforge-bridge.js",
    "team:fullstack": "node agents/techforge-spawn.js --team fullstack",
    "team:mobile": "node agents/techforge-spawn.js --team mobile",
    "team:enterprise": "node agents/techforge-spawn.js --team enterprise",
    "agent:backend": "node agents/techforge-spawn.js --expertise backend",
    "agent:frontend": "node agents/techforge-spawn.js --expertise frontend",
    "agent:database": "node agents/techforge-spawn.js --expertise database",
    "agent:security": "node agents/techforge-spawn.js --expertise security",
    "test": "node test/test-communication.js",
    "clean": "pkill -f techforge || true"
  },
  "dependencies": {
    "ws": "^8.16.0",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "chalk": "^5.3.0",
    "uuid": "^9.0.1",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "blessed": "^0.1.81",
    "form-data": "^4.0.0",
    "node-fetch": "^3.3.2",
    "minimist": "^1.2.8",
    "yaml": "^2.3.4",
    "dotenv": "^16.3.1",
    "axios": "^1.6.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}